{
  "name": "@rsickenberg/portfolio-next",
  "type": "module",
  "version": "0.0.6",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/RSickenberg/rsickenberg.me-next"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "release": "dotenv release-it"
  },
  "dependencies": {
    "@astrojs/image": "^0.16.9",
    "@astrojs/preact": "^2.2.0",
    "@astrojs/tailwind": "^3.1.3",
    "@studio-freight/lenis": "^1.0.13",
    "@tailwindcss/typography": "^0.5.9",
    "astro": "^2.5.5",
    "gsap": "^3.11.5",
    "preact": "^10.15.0",
    "tailwindcss": "^3.3.2",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "^3.1.0",
    "dotenv-cli": "^7.2.1",
    "release-it": "^15.10.3",
    "sass": "^1.62.1"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "github": {
      "release": true,
      "releaseName": "Release ${version}",
      "preRelease": false,
      "draft": false
    },
    "git": {
      "requireCleanWorkingDir": false,
      "commitMessage": "Release ${version}",
      "tagName": "v${version}",
      "tag": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md",
        "strictLatest": true,
        "addUnreleased": true,
        "addVersionUrl": true
      }
    }
  }
}
