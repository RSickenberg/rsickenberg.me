{
  "name": "@rsickenberg/portfolio-next",
  "type": "module",
  "version": "0.0.4",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/RSickenberg/rsickenberg.me-next"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "release": "dotenv release-it"
  },
  "dependencies": {
    "@astrojs/image": "^0.15.1",
    "@astrojs/preact": "^2.0.3",
    "@astrojs/tailwind": "^3.0.1",
    "@studio-freight/lenis": "^0.2.28",
    "@tailwindcss/typography": "^0.5.9",
    "astro": "^2.0.12",
    "gsap": "^3.11.4",
    "preact": "^10.12.1",
    "tailwindcss": "^3.2.6",
    "zustand": "^4.3.5"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "^3.1.0",
    "dotenv-cli": "^7.0.0",
    "release-it": "^15.6.0",
    "sass": "^1.58.1"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "github": {
      "release": true,
      "releaseName": "Release ${version}",
      "preRelease": false,
      "draft": false
    },
    "git": {
      "requireCleanWorkingDir": false,
      "commitMessage": "Release ${version}",
      "tagName": "v${version}",
      "tag": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md",
        "strictLatest": true,
        "addUnreleased": true,
        "addVersionUrl": true
      }
    }
  }
}
