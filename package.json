{
  "name": "@rsickenberg/portfolio-next",
  "type": "module",
  "version": "1.1.6",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/RSickenberg/rsickenberg.me-next"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "release": "dotenv release-it",
    "lint": "npx eslint ."
  },
  "dependencies": {
    "@astrojs/partytown": "^2.1.2",
    "@astrojs/preact": "^3.5.2",
    "@astrojs/tailwind": "^5.1.0",
    "@iconify-json/ph": "^1.2.0",
    "@sentry/astro": "^8.27.0",
    "@studio-freight/lenis": "^1.0.42",
    "@tailwindcss/typography": "^0.5.15",
    "astro": "^4.15.1",
    "astro-icon": "^1.1.1",
    "astro-seo": "^0.8.4",
    "caniuse-lite": "^1.0.30001655",
    "dayjs": "^1.11.13",
    "flowbite": "^2.5.1",
    "gsap": "^3.12.5",
    "preact": "^10.23.2",
    "tailwindcss": "^3.4.10",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "^5.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "release-it": "^17.6.0",
    "sass": "^1.77.8",
    "typescript-eslint": "^8.3.0",
    "eslint-plugin-astro": "^1.2.3",
    "@typescript-eslint/parser": "^8.3.0"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "github": {
      "release": true,
      "releaseName": "Release ${version}",
      "preRelease": false,
      "draft": false
    },
    "git": {
      "requireCleanWorkingDir": false,
      "commitMessage": "Release ${version}",
      "tagName": "v${version}",
      "tag": true,
      "requiredBranch": "main"
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md",
        "strictLatest": true,
        "addUnreleased": true,
        "addVersionUrl": true
      }
    }
  }
}
