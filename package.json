{
  "name": "@rsickenberg/portfolio-next",
  "type": "module",
  "version": "0.2.6",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/RSickenberg/rsickenberg.me-next"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "release": "dotenv release-it"
  },
  "dependencies": {
    "@astrojs/partytown": "^2.0.4",
    "@astrojs/preact": "^3.1.1",
    "@astrojs/tailwind": "^5.1.0",
    "@iconify-json/arcticons": "^1.1.98",
    "@sentry/astro": "^7.108.0",
    "@studio-freight/lenis": "^1.0.42",
    "@tailwindcss/typography": "^0.5.12",
    "astro": "^4.5.10",
    "astro-icon": "^1.1.0",
    "gsap": "^3.12.5",
    "preact": "^10.20.1",
    "tailwindcss": "^3.4.3",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "^5.0.0",
    "dotenv-cli": "^7.3.0",
    "release-it": "^17.0.0",
    "sass": "^1.70.0"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "github": {
      "release": true,
      "releaseName": "Release ${version}",
      "preRelease": true,
      "draft": false
    },
    "git": {
      "requireCleanWorkingDir": false,
      "commitMessage": "Release ${version}",
      "tagName": "v${version}",
      "tag": true,
      "requiredBranch": "main"
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md",
        "strictLatest": true,
        "addUnreleased": true,
        "addVersionUrl": true
      }
    }
  }
}
