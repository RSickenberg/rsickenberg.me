{
  "name": "@rsickenberg/portfolio-next",
  "type": "module",
  "version": "1.2.9",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/RSickenberg/rsickenberg.me-next"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "release": "dotenv release-it",
    "lint": "npx eslint ."
  },
  "dependencies": {
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/preact": "^4.1.3",
    "@astrojs/sitemap": "^3.7.0",
    "@iconify-json/ph": "^1.2.2",
    "@sentry/astro": "^10.36.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.14",
    "astro-icon": "^1.1.5",
    "astro-seo": "^1.1.0",
    "astro-tailwind-config-viewer": "^2.0.2",
    "caniuse-lite": "^1.0.30001765",
    "dayjs": "^1.11.19",
    "flowbite": "^4.0.1",
    "gsap": "^3.14.2",
    "lenis": "^1.3.17",
    "preact": "^10.28.2",
    "tailwindcss": "^4.1.18",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "^7.0.0",
    "@typescript-eslint/parser": "^8.53.1",
    "dotenv-cli": "^11.0.0",
    "esbuild-plugin-browserslist": "^2.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "globals": "^17.0.0",
    "release-it": "^19.2.4",
    "sass-embedded": "^1.97.3",
    "typescript-eslint": "^8.53.1"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "github": {
      "release": true,
      "releaseName": "Release ${version}",
      "preRelease": false,
      "draft": false
    },
    "git": {
      "requireCleanWorkingDir": false,
      "commitMessage": "Release ${version}",
      "tagName": "v${version}",
      "tag": true,
      "requiredBranch": "main"
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md",
        "strictLatest": true,
        "addUnreleased": true,
        "addVersionUrl": true
      }
    }
  },
  "browserslist": [
    "defaults, last 5 versions, not dead"
  ],
  "trustedDependencies": [
    "@sentry/cli",
    "@tailwindcss/oxide",
    "esbuild",
    "sharp"
  ]
}
